{
  "MongoDB": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "archive",
    "CollectionName": "alerts"

  },
  "Redis": {
    "Configuration": {
      "ServerAdress": "localhost:6379"
    }
  },
  "Consumer": {
    "Configuration": {
      "BootstrapServers": "127.0.0.1:29092"
    },
    "TopicName": "telemetry-data"
  },
  "SensorsProperties": {
    "DocumentName": "Requirements"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
